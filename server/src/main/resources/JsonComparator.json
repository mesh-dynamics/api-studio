{
    "test1" : {
        "json1": {
            "hdr":  { "h1": "h1v1",      "h2": 10,           "h3": 5.456 },
            "body": { "b1": "test123",   "b2": [ 1, 3, 3] }
        },
        "json2": {
            "hdr":  { "h1": "h1v1",      "h2": 10,           "h3": 5.458 },
            "body": { "b1": "test456",   "b2": [ 1, 2, 3] },
            "b3":   { "a1": "a1v1",      "a2": 15 }
        },
        "output" : {
            "mt":           "FuzzyMatch",
            "matchmeta":    "JsonDiff",
            "diffs": [
                { "op": "replace",  "path": "/hdr/h3",      "value": 5.458,     "fromValue":5.456,      "resolution":"OK" },
                { "op": "replace",  "path": "/body/b1",     "value": "test456", "fromValue":"test123",  "resolution":"OK" },
                { "op": "add",      "path": "/body/b2/1",   "value": 2,                                 "resolution":"OK" },
                { "op": "remove",   "path": "/body/b2/3",   "value": 3,                                 "resolution":"OK" },
                { "op": "add",      "path": "/b3",          "value": { "a1": "a1v1", "a2": 15 },        "resolution":"OK" }
            ]
        }
    },

    "test2": {
        "output": {
            "mt":           "NoMatch",
            "matchmeta":    "JsonDiff",
            "diffs": [
                { "op":"replace",   "path":"/hdr/h3",       "value": 5.458,     "fromValue":5.456,      "resolution":"ERR_ValMismatch"      },
                { "op":"replace",   "path":"/body/b1",      "value": "test456", "fromValue":"test123",  "resolution":"ERR_ValMismatch"      },
                { "op":"add",       "path":"/body/b2/1",    "value": 2,                                 "resolution":"OK_OtherValInvalid"   },
                { "op":"remove",    "path":"/body/b2/3",    "value": 3,                                 "resolution":"OK"                   },
                { "op":"add",       "path":"/b3",           "value": { "a1": "a1v1", "a2": 15 },        "resolution":"OK_OtherValInvalid"   }
            ]
        }
    },

    "test3": {
        "output": {
            "mt":           "ExactMatch",
            "matchmeta":    "JsonDiff",
            "diffs": []
        }
    },

    "test4" : {
        "json1": {
            "hdr":  { "h1": "h1v1",      "h2": 10,           "h3": 5.456 },
            "body": { "b1": "test123",   "b2": [ 1, 3, 3] }
        },
        "json2": {
            "hdr":  { "h1": "h1v1",      "h2": 10,           "h3": 5.456 },
            "body": { "b1": "test123" }
        },
        "output" : {
            "mt":           "FuzzyMatch",
            "matchmeta":    "JsonDiff",
            "diffs": [
                { "op": "remove", "path":"/body/b2", "value": [ 1, 3, 3], "resolution": "OK" }
            ]
        }
    },

    "test5": {
        "output": {
            "mt":           "FuzzyMatch",
            "matchmeta":    "JsonDiff",
            "diffs": [
                { "op": "remove", "path": "/body/b2", "value": [ 1, 3, 3], "resolution":"OK_Optional" }
            ]
        }
    },

    "test6": {
        "output": {
            "mt":           "NoMatch",
            "matchmeta":    "JsonDiff",
            "diffs": [
                { "op": "noop",     "path": "/body/b2",  "value": null,         "resolution": "ERR_Required" },
                { "op": "remove",   "path": "/body/b2",  "value": [ 1, 3, 3],   "resolution": "ERR_Required" }
            ]
        }
    },

    "test7" : {
        "json1": {
            "string":   "5c80e878323659a64d123db4",
            "int":      35,
            "float":    41.280324,
            "obj":      { "name": "Steele Dominguez" },
            "rptArr":   [ "dolore", "irure" ],
            "nrptArr":  [
                { "id": 0 },
                { "name": "Steele Dominguez"}
            ]
        },
        "json2": {
            "string":   123,
            "int":      "35",
            "float":    41,
            "obj":      "not an object",
            "rptArr":   123,
            "nrptArr":  [ "string", 123 ]
        },
        "output" : {
            "mt":           "NoMatch",
            "matchmeta":    "JsonDiff",
            "diffs": [
                { "op": "noop",     "path": "/int",         "value": "35",                                                              "resolution":"ERR_ValTypeMismatch" },
                { "op": "noop",     "path": "/rptArr",      "value": 123,                                                               "resolution":"ERR_ValTypeMismatch" },
                { "op": "noop",     "path": "/string",      "value": 123,                                                               "resolution":"ERR_ValTypeMismatch" },
                { "op": "noop",     "path": "/float",       "value": 41,                                                                "resolution":"ERR_ValTypeMismatch" },
                { "op": "noop",     "path": "/obj",         "value": "not an object",                                                   "resolution":"ERR_ValTypeMismatch" },
                { "op": "replace",  "path": "/string",      "value": 123,               "fromValue": "5c80e878323659a64d123db4",        "resolution":"ERR_ValTypeMismatch" },
                { "op": "replace",  "path": "/int",         "value": "35",              "fromValue": 35,                                "resolution":"ERR_ValTypeMismatch" },
                { "op": "replace",  "path": "/float",       "value": 41,                "fromValue": 41.280324,                         "resolution":"ERR_ValTypeMismatch" },
                { "op": "replace",  "path": "/obj",         "value": "not an object",   "fromValue": { "name": "Steele Dominguez" },    "resolution":"ERR_ValTypeMismatch" },
                { "op": "replace",  "path": "/rptArr",      "value": 123,               "fromValue": [ "dolore", "irure" ],             "resolution":"ERR_ValTypeMismatch" },
                { "op": "replace",  "path": "/nrptArr/0",   "value": "string",          "fromValue": { "id": 0 },                       "resolution":"ERR_ValTypeMismatch" },
                { "op": "replace",  "path": "/nrptArr/1",   "value": 123,               "fromValue": { "name": "Steele Dominguez" },    "resolution":"ERR_ValTypeMismatch" }
            ]
        }
    }
}
