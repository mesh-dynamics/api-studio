  "scripts": {
    "build": "webpack --mode production  --display minimal --config webpack.web.config.js",
    "build:dev": "webpack --mode development --display minimal --config webpack.web.config.js",
    "build:electron:js": "webpack --mode production --display minimal --config webpack.electron.config.js",
    "build:electron:js:dev": "webpack --mode development --display minimal --config webpack.electron.config.js",
    "build:electron": "electron-builder build --config electron-builder.yml",
    "electron": "electron .",
    "package": "npm run build:electron:js && electron-builder build -mwl --config electron-builder.yml --publish never",
    "package:deploy": "npm run build:electron:js && electron-builder build -mwl --config electron-builder.yml --publish always",
    "package:dev": "npm run build:electron:js:dev && electron-builder build --config electron-builder.yml",
    "package:prod": "npm run build:electron:js && electron-builder build --config electron-builder.yml",
    "postinstall": "electron-builder install-app-deps",
    "start": "webpack-dev-server --config webpack.web.config.js",
    "stop": "node ./stop-electron.js",
    "watch:js": "watch \"npm run build:electron:js:dev\" ./src --wait=5",
    "watch:js:prod": "watch \"npm run build:electron:js\" ./src --wait=5",
    "watch:electron:js": "watch \"npm stop && npm run build:electron:js && npm run electron\" ./src --wait=5",
    "watch:electron:js:dev": "watch \"npm stop && npm run build:electron:js:dev && npm run electron\" ./src --wait=5"
  }
