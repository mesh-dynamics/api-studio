{
  "name": "cube-ui",
  "productName": "Mesh Dynamics",
  "description": "Web app for cube ui",
  "version": "0.2.14",
  "private": true,
  "main": "build/electron.js",
  "homepage": "./",
  "author": "Mesh Dynamics <developers@meshdynamics.io>",
  "build": {
    "appId": "com.meshdynamics.cubeui",
    "extraResources": [
      "./assets/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "cube-io-corp",
        "repo": "cubeui",
        "private": true,
        "token": "5399d6cde7a6b57fe2ce9bcc22126e139d5581c6"
      }
    ],
    "files": [
      "build/**/*",
      "build/assets/images/**/*",
      "build/assets/images/md-circle-logo.png",
      "node_modules/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/mesh-favicon.ico",
      "legalTrademarks": "2020-21 meshdynamics.io"
    },
    "linux": {
      "target": "deb",
      "executableName": "Cube",
      "icon": "public/mesh-favicon.png",
      "maintainer": "Mesh Dynamics"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.developer-tools",
      "icon": "public/mesh-favicon.icns"
    }
  },
  "dependencies": {
    "@tippy.js/react": "^2.2.2",
    "array-to-tree": "^3.3.2",
    "axios": "^0.18.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "cytoscape": "^3.7.3",
    "cytoscape-canvas": "^3.0.1",
    "cytoscape-cola": "^2.3.0",
    "cytoscape-cose-bilkent": "^4.0.0",
    "cytoscape-dagre": "^2.2.2",
    "cytoscape-klay": "^3.1.2",
    "cytoscape-popper": "^1.0.4",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.1",
    "electron-updater": "^4.3.1",
    "express": "^4.16.4",
    "file-saver": "^2.0.0",
    "formik": "^1.5.7",
    "http-proxy": "^1.18.1",
    "is-electron": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "plotly.js": "^1.42.5",
    "prop-types": "^15.6.2",
    "query-string": "5.1.1",
    "react": "^16.6.0",
    "react-ace": "^8.1.0",
    "react-bootstrap": "^0.32.4",
    "react-datepicker": "^2.9.6",
    "react-dom": "^16.6.0",
    "react-google-recaptcha": "^2.0.1",
    "react-idle-timer": "^4.2.12",
    "react-iframe": "^1.8.0",
    "react-plotly.js": "^2.2.0",
    "react-redux": "^5.0.7",
    "react-resize-detector": "^4.2.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-select": "^2.0.0-beta.7",
    "react-table": "^6.10.0",
    "react-table-hoc-fixed-columns": "^2.1.1",
    "react-tiny-popover": "^4.0.0",
    "recharts": "^1.5.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.1.1",
    "styled-components": "^4.3.2",
    "tippy.js": "^4.3.5",
    "validator": "^12.1.0",
    "wait-on": "^5.0.0"
  },
  "devDependencies": {
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cube-io-corp/cubeui.git"
  },
  "scripts": {
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3006 && electron .\"",
    "electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "start": "cross-env PORT=3006 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "start:electron": "npm run build && concurrently \"npm run start\" \"npm run electron\"",
    "build:electron": "npm run build && npm run electron",
    "build:package": "npm run build && npm run dist",
    "package:deploy:win": "npm run build && electron-builder build --win --publish always",
    "package:deploy:mac": "npm run build && electron-builder build --mac --publish always"
  }
}